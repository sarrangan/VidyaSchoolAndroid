    <!-- Narayana Suktam Lyrics -->
    <string-array name="narayana_suktam_lines_all">
        <item>sahasraśīrṣaṁ devaṁ viśvākśaṁ viśvaśambhuvaṁ</item>
        <item>viśvaṁ nārāyaṇaṁ devamakśaraṁ paramaṁ padam</item>
